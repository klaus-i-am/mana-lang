// Arrays and vectors test
module test_arrays;

fn main() -> i32 {
    // Fixed-size array
    let arr: [i32; 5] = [1, 2, 3, 4, 5];
    println(arr[0]);
    // expect: 1
    println(arr[2]);
    // expect: 3
    println(arr[4]);
    // expect: 5

    // Vector (dynamic array)
    let mut vec: Vec<i32> = Vec::new();
    vec.push(10);
    vec.push(20);
    vec.push(30);

    println(vec[0]);
    // expect: 10
    println(vec[1]);
    // expect: 20
    println(vec.len());
    // expect: 3

    // Iterate over array
    for x in arr {
        print(x);
        print(" ");
    }
    println("");
    // expect: 1 2 3 4 5

    // Array with fill
    let filled: [i32; 3] = [42; 3];
    println(filled[0]);
    // expect: 42
    println(filled[1]);
    // expect: 42
    println(filled[2]);
    // expect: 42

    return 0;
}
