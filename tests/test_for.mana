module test_for

fn main() -> i32 {
    // Traditional for loop
    println("Counting 0 to 4:")
    for i: i32 = 0; i < 5; i++ {
        println(i)
    }

    // Nested for loops
    println("Multiplication table 1-3:")
    for i: i32 = 1; i <= 3; i++ {
        for j: i32 = 1; j <= 3; j++ {
            println(i * j)
        }
    }

    return 0
}
