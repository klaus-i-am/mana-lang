module main;

trait Shape {
    fn area(self) -> f64;
}

struct Circle {
    radius: f64;
}

impl Shape for Circle {
    fn area(self) -> f64 {
        return 3.14159 * self.radius * self.radius;
    }
}

fn main() -> i32 {
    let c = Circle { radius: 5.0 };
    let a = c.area();
    println(a);
    return 0;
}
