module range_test;

fn main() -> i32 {
    println("=== Range Test ===");
    println();

    // Exclusive range: 0..5 = [0, 1, 2, 3, 4]
    print("0..5: ");
    for i in 0..5 {
        print(i);
        print(" ");
    }
    println();

    // Inclusive range: 1..=5 = [1, 2, 3, 4, 5]
    print("1..=5: ");
    for i in 1..=5 {
        print(i);
        print(" ");
    }
    println();

    // Range with start
    print("5..10: ");
    for i in 5..10 {
        print(i);
        print(" ");
    }
    println();

    println();
    println("=== Range tests passed! ===");
    return 0;
}
