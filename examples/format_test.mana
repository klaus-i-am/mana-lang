module format_test

// Note: F-string format specifiers have been replaced with variadic println
// For advanced formatting, use string manipulation functions

fn main() -> i32 {
    println("=== Variadic Print Test ===")
    println()

    // 1. Float values
    println("1. Float printing:")
    let pi: float = 3.14159265359
    println("  pi = ", pi)
    println()

    // 2. Integer values
    println("2. Integer printing:")
    let num: i32 = 42
    println("  num = ", num)
    let small: i32 = 7
    println("  small = ", small)
    println()

    // 3. Multiple values
    println("3. Multiple values:")
    let x: i32 = 5
    let y: float = 3.14159
    println("  x = ", x, ", y = ", y)
    println()

    // 4. Expressions
    println("4. Expressions:")
    let a: i32 = 10
    let b: i32 = 20
    println("  ", a, " + ", b, " = ", a + b)
    println()

    // 5. String values
    println("5. String printing:")
    let name: string = "Alice"
    println("  name = ", name)
    println()

    // 6. Boolean values
    println("6. Boolean printing:")
    let flag: bool = true
    println("  flag = ", flag)
    println()

    println("=== All variadic print tests passed! ===")
    return 0
}
