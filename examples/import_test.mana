module main;

import "math_lib";

fn main() -> i32 {
    sum: i32 = add(10, 20);
    println(sum);  // 30

    product: i32 = multiply(5, 6);
    println(product);  // 30

    v1: Vector2 = Vector2{x: 3, y: 4};
    v2: Vector2 = Vector2{x: 1, y: 2};
    dot: i32 = v1.dot(v2);
    println(dot);  // 3*1 + 4*2 = 11

    return sum;
}
